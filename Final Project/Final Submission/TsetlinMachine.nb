Cell[BoxData[
 RowBox[{
  RowBox[{"TsetlinOutputCalculateFormula", "[", 
   RowBox[{"to_TsetlinOutput", ",", "si_Integer"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{"TsetlinOutputGetFunction", "[", "to", "]"}], "[", 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"And", "@@", 
      RowBox[{"{", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"And", "@@", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"MapThread", "[", "\[IndentingNewLine]", 
               RowBox[{
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"(", 
                    RowBox[{
                    RowBox[{"TsetlinAutomatonCalculateAction", "[", 
                    RowBox[{"#1", ",", "si"}], "]"}], "-", "1"}], ")"}], "*", 
                   "#2"}], ")"}], "&"}], ",", "\[IndentingNewLine]", 
                RowBox[{"{", "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{"TsetlinClauseGetPositive", "[", "#", "]"}], ",", 
                  "\[IndentingNewLine]", 
                  RowBox[{"Range", "@", 
                   RowBox[{"Length", "@", 
                    RowBox[{"TsetlinClauseGetPositive", "[", "#", "]"}]}]}]}],
                  "}"}]}], "]"}], "&"}], "@", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"TsetlinOutputGetClauses", "[", "to", "]"}], 
              "\[LeftDoubleBracket]", "#", "\[RightDoubleBracket]"}]}], ")"}],
            "/.", 
           RowBox[{"{", 
            RowBox[{"0", "\[Rule]", "Nothing"}], "}"}]}], ")"}]}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"And", "@@", 
         RowBox[{"(", 
          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"MapThread", "[", "\[IndentingNewLine]", 
               RowBox[{
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"-", 
                    RowBox[{"(", 
                    RowBox[{
                    RowBox[{"TsetlinAutomatonCalculateAction", "[", 
                    RowBox[{"#1", ",", "si"}], "]"}], "-", "1"}], ")"}]}], 
                   "*", "#2"}], ")"}], "&"}], ",", "\[IndentingNewLine]", 
                RowBox[{"{", "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{"TsetlinClauseGetNegative", "[", "#", "]"}], ",", 
                  "\[IndentingNewLine]", 
                  RowBox[{"Range", "@", 
                   RowBox[{"Length", "@", 
                    RowBox[{"TsetlinClauseGetNegative", "[", "#", "]"}]}]}]}],
                  "}"}]}], "]"}], "&"}], "@", "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"TsetlinOutputGetClauses", "[", "to", "]"}], 
              "\[LeftDoubleBracket]", "#", "\[RightDoubleBracket]"}]}], ")"}],
            "/.", 
           RowBox[{"{", 
            RowBox[{"0", "\[Rule]", "Nothing"}], "}"}]}], ")"}]}]}], "}"}]}], 
     "&"}], "/@", "\[IndentingNewLine]", 
    RowBox[{"Range", "@", 
     RowBox[{"Length", "@", 
      RowBox[{"TsetlinOutputGetClauses", "[", "to", "]"}]}]}]}], 
   "]"}]}]], "Input",
 InitializationCell->True,
 CellLabel->"In[15]:=",ExpressionUUID->"68bfa9d9-3314-48a4-b3d7-84a35dcdd6b5"],

