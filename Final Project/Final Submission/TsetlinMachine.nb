Cell[BoxData[
 RowBox[{
  RowBox[{"TsetlinClauseCalculateIncludedInputs", "[", 
   RowBox[{"tc_TsetlinClause", ",", "si_Integer", ",", "li_List"}], "]"}], ":=",
   "\[IndentingNewLine]", 
  RowBox[{"Last", "/@", "\[IndentingNewLine]", 
   RowBox[{"Select", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Join", "[", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Transpose", "[", 
        RowBox[{"{", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"TsetlinAutomatonCalculateAction", "[", 
              RowBox[{"#", ",", "si"}], "]"}], "&"}], "/@", 
            RowBox[{"TsetlinClauseGetPositive", "[", "tc", "]"}]}], ")"}], 
          ",", "\[IndentingNewLine]", "li"}], "}"}], "]"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"Transpose", "[", 
        RowBox[{"{", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"(", 
           RowBox[{
            RowBox[{
             RowBox[{"TsetlinAutomatonCalculateAction", "[", 
              RowBox[{"#", ",", "si"}], "]"}], "&"}], "/@", 
            RowBox[{"TsetlinClauseGetNegative", "[", "tc", "]"}]}], ")"}], 
          ",", "\[IndentingNewLine]", 
          RowBox[{"Not", "/@", "li"}]}], "}"}], "]"}]}], "]"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"#", "[", 
        RowBox[{"[", "1", "]"}], "]"}], "\[Equal]", "2"}], "&"}]}], 
    "]"}]}]}]], "Input",
