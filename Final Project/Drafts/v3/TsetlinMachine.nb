          RowBox[{
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"TsetlinAction", "[", 
               RowBox[{"#", ",", "si"}], "]"}], "&"}], "/@", 
             RowBox[{"TsetlinClauseNonNegated", "[", "tc", "]"}]}], ")"}], 
           ",", "\[IndentingNewLine]", "li"}], "}"}], "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"Transpose", "[", 
         RowBox[{"{", "\[IndentingNewLine]", 
          RowBox[{
