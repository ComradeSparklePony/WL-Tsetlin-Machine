   RowBox[{"t_Integer", ",", "si_Integer", ",", "n_Integer"}], "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"If", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"TsetlinAction", "[", 
      RowBox[{"t", ",", "si"}], "]"}], "\[Equal]", "n"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{"TsetlinReward", "[", 
     RowBox[{"t", ",", "si"}], "]"}], ",", "\[IndentingNewLine]", 
    RowBox[{"TsetlinPunish", "[", 
     RowBox[{"t", ",", "si"}], "]"}]}], "]"}]}]], "Input",
