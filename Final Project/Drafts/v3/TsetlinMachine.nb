 " (unless ",
 StyleBox["x",
  FontSlant->"Italic"],
 "==",
 StyleBox["y",
  FontSlant->"Italic"],
 ", in which case ",
 StyleBox["x",
  FontSlant->"Italic"],
 " is outputted)."
}], "Text",
