    RowBox[{"y_", "?", "NumberQ"}], ",", 
    RowBox[{"t_", "?", "NumberQ"}]}], "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Which", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"x", "\[Equal]", "y"}], ",", "x", ",", "\[IndentingNewLine]", 
    RowBox[{"x", ">", "y"}], ",", 
    RowBox[{"x", "-", "t"}], ",", "\[IndentingNewLine]", 
