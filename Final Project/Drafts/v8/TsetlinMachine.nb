       RowBox[{"IntegerQ", "[", "sc", "]"}], ",", "0", ",", 
       "\[IndentingNewLine]", 
       RowBox[{"sc", ">", "1"}], ",", 
       RowBox[{"AbsoluteTime", "[", "]"}]}], "]"}]}], ";", 
    "\[IndentingNewLine]", 
    RowBox[{"NestWhile", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{"trainInput", ",", "trainOutput"}], "}"}], "=", 
